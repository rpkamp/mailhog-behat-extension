{
    "name": "rpkamp/mailhog-behat-extension",
    "description": "Mailhog Extension for Behat",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Remon van de Kamp",
            "email": "rpkamp@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "rpkamp\\Behat\\MailhogExtension\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "rpkamp\\Behat\\MailhogExtension\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "php": "^7.2 || ^8.0",
        "behat/behat": "^3.8.0",
        "rpkamp/mailhog-client": "^0.5.2",
        "php-http/discovery": "^1.3",
        "symfony/dependency-injection": "^4.4 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "guzzlehttp/psr7": "^1.6",
        "php-parallel-lint/php-parallel-lint": "^1.2",
        "php-http/curl-client": "^2.0",
        "swiftmailer/swiftmailer": "^6.2",
        "phpunit/phpunit": "^8.3.4",
        "mockery/mockery": "^1.3.3",
        "phpstan/phpstan": "^0.12.64",
        "phpstan/phpstan-mockery": "^0.12.11",
        "doctrine/coding-standard": "^8.0",
        "egulias/email-validator": "^2.1.23",
        "phpmd/phpmd": "^2.9.1",
        "nyholm/psr7": "^1.3"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
